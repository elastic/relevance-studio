{
  "_meta": {
    "description": "Elasticsearch Relevance Studio - Projects"
  },
  "index_patterns": [
    "esrs-projects*"
  ],
  "template": {
    "mappings": {
      "dynamic": "strict",
      "properties": {
        "name": {
          "type": "keyword"
        },
        "index_pattern": {
          "type": "keyword"
        },
        "params": {
          "type": "keyword"
        },
        "rating_scale": {
          "properties": {
            "min": {
              "type": "byte"
            },
            "max": {
              "type": "byte"
            }
          }
        }
      }
    }
  }
}
{
  "_meta": {
    "description": "Elasticsearch Relevance Studio - Strategies"
  },
  "index_patterns": [
    "esrs-strategies*"
  ],
  "template": {
    "mappings": {
      "dynamic": "strict",
      "properties": {
        "@timestamp": {
          "type": "keyword"
        },
        "project_id": {
          "type": "keyword"
        },
        "name": {
          "type": "keyword"
        },
        "params": {
          "type": "keyword"
        },
        "tags": {
          "type": "keyword"
        },
        "template": {
          "type": "object",
          "enabled": false
        },
        "_search": {
          "properties": {
            "name": {
              "type": "text"
            },
            "params": {
              "type": "text"
            },
            "tags": {
              "type": "text"
            },
            "template": {
              "type": "text"
            }
          }
        }
      }
    }
  }
}
{
  "_meta": {
    "description": "Elasticsearch Relevance Studio - Scenarios"
  },
  "index_patterns": [
    "esrs-scenarios*"
  ],
  "template": {
    "mappings": {
      "dynamic": "strict",
      "properties": {
        "@timestamp": {
          "type": "keyword"
        },
        "project_id": {
          "type": "keyword"
        },
        "name": {
          "type": "keyword"
        },
        "params": {
          "type": "keyword"
        },
        "tags": {
          "type": "keyword"
        },
        "values": {
          "type": "object",
          "enabled": false
        }
      }
    }
  }
}
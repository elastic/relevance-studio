{
  "_meta": {
    "description": "Elasticsearch Relevance Studio - Judgements"
  },
  "index_patterns": [
    "esrs-judgements*"
  ],
  "template": {
    "mappings": {
      "dynamic": "strict",
      "properties": {
        "@meta": {
          "properties": {
            "created_at": {
              "type": "date"
            },
            "created_by": {
              "type": "keyword"
            },
            "updated_at": {
              "type": "date"
            },
            "updated_by": {
              "type": "keyword"
            }
          }
        },
        "workspace_id": {
          "type": "keyword"
        },
        "scenario_id": {
          "type": "keyword"
        },
        "index": {
          "type": "keyword"
        },
        "doc_id": {
          "type": "keyword"
        },
        "rating": {
          "type": "byte"
        },
        "_search": {
          "properties": {
            "index": {
              "type": "text"
            }
          }
        }
      }
    }
  }
}
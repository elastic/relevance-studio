{
  "_meta": {
    "description": "Elasticsearch Relevance Studio - Displays"
  },
  "index_patterns": [
    "esrs-displays*"
  ],
  "template": {
    "mappings": {
      "dynamic": "strict",
      "properties": {
        "project_id": {
          "type": "keyword"
        },
        "index_pattern": {
          "type": "keyword"
        },
        "fields": {
          "type": "keyword"
        },
        "template": {
          "type": "object",
          "enabled": false
        },
        "_search": {
          "properties": {
            "index_pattern": {
              "type": "text"
            },
            "fields": {
              "type": "text"
            },
            "template": {
              "type": "text"
            }
          }
        }
      }
    }
  }
}